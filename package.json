    "url": "git+https://github.com/yamitsushi/esbuild-plugin-env.git"
  },
  "bugs": {
    "url": "https://github.com/yamitsushi/esbuild-plugin-env/issues"
  },
  "homepage": "https://github.com/yamitsushi/esbuild-plugin-env#readme",
